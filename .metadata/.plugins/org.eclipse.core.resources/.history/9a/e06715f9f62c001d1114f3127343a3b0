package sec1;

public class Oper3 {

	public static void main(String[] args) {
		// 문자열 결합
		byte a='k', b='i', c='m';
		int d = 1004;
		System.out.println("a="+a);
		System.out.println("b="+b);
		System.out.println("c="+c);
		System.out.println(a+b+c+1004); //1325 도출됨 k의 키값이
		System.out.println("kim"+d);
	}

}
