package sec2;

public class Television implements RemoteControl{
	private int volume;

	public int getVolume() {return volume;}

	public void setVolume(int volume) {this.volume = volume;}

	@Override
	public void turnOn() {
		System.out.println("TV 전원을 켭니다.");
		
	}
	@Override
	public void turnOff() {
		System.out.println("TV전원을 끕니다.");
	}
	

}
