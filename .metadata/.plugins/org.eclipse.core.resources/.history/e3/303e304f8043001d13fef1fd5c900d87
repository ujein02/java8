package sec1;
//https://docs.oracle.com/javase/8/docs/api/
public class Api1 {

	public static void main(String[] args) {
		// java.lang
		/*
		 Wrapper : primitive type은 메서드를 가질 수 없으므로 
		
		*/
		
		int a=1004;
		Integer b = new Integer(1004);
		
		//char	<=> Character
		//float	<=>	Float
		//double	<=>	Double
		//long	<=>	Long
		//boolean	<=>	Boolean

	}

}
