package sec3;

public class Loop8 {

	public static void main(String[] args) {
		// 이중 for문을 활용하여 아래와 같은 형태의 구구단을 출력
/*
 *  2*1=2	3*1=3 ,.............9*1=9
 *  2*2=4	3*2=6 ,.............9*2
 *  ...............................
 *  2*9=18 ,....................9*9=81		
 */
		for(int i=2;i<=9;i++){
			for(int n=1;n<=9;n++){
			System.out.print(i+"*"+n+"="+(i*n)+"  ");
			}
		System.out.println();	
		}

	}

}
