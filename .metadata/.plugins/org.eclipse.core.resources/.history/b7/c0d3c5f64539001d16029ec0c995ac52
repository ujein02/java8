package sec2;

public class Student {
	String name;
	int kor;
	int eng;
	int mat;
	
	public Student() { }
	public Student(String name){this.name = name;}
	public Student(String name, int kor, int eng, int mat){
		this.name = name; this.kor = kor; this.eng = eng; this.mat = mat;
	}
